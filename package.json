{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "cross-env NODE_ENV=development node src/server/app.js",
    "prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.config.js",
    "build": "webpack --mode production --config webpack/webpack.prod.config.js",
    "start": "webpack-dev-server --mode development --config webpack/webpack.dev.config.js --hot --inline",
    "test": "jest",
    "cypress:open": "cypress open"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/client/app/mocks/**",
      "!src/client/app/components/ErrorBoundary/**",
      "!src/server/**"
    ],
    "coverageDirectory": "tests",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest/config.js"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.1",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "morgan": "~1.9.0",
    "postcss-loader": "^2.1.4",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^4.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "cypress": "^2.1.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "react-test-renderer": "^16.3.2",
    "style-loader": "^0.21.0"
  }
}
